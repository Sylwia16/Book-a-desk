<h2>Mapa biura</h2>

 <%= image_tag "3.png", width: 600, class: 'd-block mx-auto' %>

 <p>Aktualnie dostÄ™pne biurka:</p>


 <div class="btn-toolbar mb-2" role="toolbar" aria-label="Toolbar with button groups">
  <% Desk.all.each do |desk| %>
    <div class="btn-group me-5" role="group" aria-label="Basic radio button group">
      <% if desk.availability %>
        <input type="radio" class="btn-check" id="btncheck<%= desk.DeskID %>" data-toggle-id="btncheck<%= desk.DeskID %>">
        <label class="btn btn-success" for="btncheck<%= desk.DeskID %>"><%= desk.DeskID %></label>
      <% else %>
        <input type="radio" class="btn-check" id="btncheck<%= desk.DeskID %>" data-toggle-id="btncheck<%= desk.DeskID %>" >
        <label class="btn btn-danger" for="btncheck<%= desk.DeskID %>"><%= desk.DeskID %></label>
      <% end %>
    </div>
  <% end %>
</div>

<div class="btn btn-success">
  <%= link_to 'Zarezerwuj biurko', bookings_path, :class => "btn btn-success"%>
</div>

